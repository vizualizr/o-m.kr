@article{jacobsenBrainCorrelatesAesthetic2006,
  title = {Brain {{Correlates}} of {{Aesthetic Judgment}} of {{Beauty}}},
  author = {Jacobsen, Thomas and Schubotz, Ricarda I. and Höfel, Lea and v. Cramon, D. Yves},
  year = 2006,
  month = jan,
  journal = {NeuroImage},
