@book{zielinskiDeepTimeMedia2008,
  title = {Deep {{Time}} of the {{Media}}: {{Toward}} an {{Archaeology}} of {{Hearing}} and {{Seeing}} by {{Technical Means}}},
  shorttitle = {Deep {{Time}} of the {{Media}}},
  author = {Zielinski, Siegfried and Druckrey, Timothy},
