@article{baeckerSocietyDigitalMedia,
  title = {In the {{Society}} of {{Digital Media}}},
  author = {Baecker, Dirk},
  abstract = {The society of electronic and digital media produces an “overflow of meaning” (Niklas Luhmann) in relation to the structure and culture of earlier media epochs of human society. The article recalls these earlier media epochs, i.e. the epochs of orality (“tribal society”), writing (“ancient high culture”) and printing (“modern society”), to explain the theorem of an overflow of meaning and to ask about the specific meaning overflow of the society of electronic and digital media. This overflow – ‘surplus’ to some, ‘excess’ to others –is described as a control overflow, whereby the instantaneous communication of news, sound and moving images of electronic media and the memory, algorithms and programs of digital media accompany every possible communication of any content among between any participants, on the one hand, and which is responded to by humans and their social systems developing ever new ways of exploring and exploiting this control, on the other. A new structure of society distributes this overflow to different areas of society, each of which finds its own way of dealing with electronic and digital media. And a new culture of society is condensing the overflow into a recurring meaning throughout society. We are probably currently witnessing the emergence of a structure of network society and a culture of complexity, i.e. the search for new forms of synchronisation of body, consciousness, communication and technology. The article concludes with some remarks on the question of communication with machines.},
  langid = {english},
  file = {D:\yonggeun\porter\zotero\storage\PCT9X3IW\Baecker - In the Society of Digital Media.pdf}
}

