  langid = {english},
  keywords = {Gardening / Garden Design,Gardening / Ornamental Plants,Gardening / Regional / General,Philosophy,Philosophy / Criticism,Philosophy / General,Philosophy / History & Surveys / Modern,Philosophy German,Social Science / General},
