  shorttitle = {Posthuman {{Bodies}}},
  booktitle = {Mapping the {{Posthuman}}},
  author = {Hayles, N. Katherine},
  year = 2023,
  publisher = {Routledge},
  abstract = {Emphasizing the common capacity for cognition, this chapter presents an integrated framework that includes humans, nonhuman biological lifeforms, and computational media. It clearly distinguishes between the agential powers of material processes, which are the result of deterministic physico-chemical forces, and the capacities for choice, selection and novelty that all biological lifeforms exhibit. It argues that each lifeform has a characteristic Umwelt or worldview, created by the ways in which it processes internal and external information from the environment. The chapter extends this argument to computational media, which also have characteristic ways of responding to informational input and generating outputs. Acknowledging that at the level of logic gates computational media are almost completely deterministic, it shows how introducing pseudo-randomness and recursivity complicate computational programs, enabling them to produce evolutionary approximations for problems with no explicit solution. Finally, it explores the possibilities of contemporary neural nets based on transformer architectures, suggesting that the textual productions of these large models reflect the worldview of the programs and are appropriate objects for literary analysis.},
  isbn = {978-1-003-32260-3},
  file = {D:\yonggeun\porter\zotero\storage\RH9W2G5F\Hayles - 2023 - Posthuman Bodies Why They (Still) Matter.pdf}
}

