@article{gangDijainyiMunhwajeogIhaereul2011,
  title = {디자인의 문화적 이해를 위한 사회이론적 탐구: 파슨스의 문화이론을 통해서 본 디자인과 문화, 그리고 기능의 문제},
  shorttitle = {디자인의 문화적 이해를 위한 사회이론적 탐구},
  author = {강, 정민},
  year = 2011,
  journal = {인문학연구},
  volume = {42},
  number = {0},
  pages = {389--412},
  issn = {1598-9259},
  url = {http://www.papersearch.net/view/detail.asp?detail_key=1i800208},
  urldate = {2017-04-19},
  abstract = {이 논문은 디자인과 같은 실천영역의 문화적 이해를 위해서는 엄격한 개념적 설명에 기초한 사회, 문화이론의 도입이 필요하다는 문제의식에서 출발하여, 구조기능주의자이면서 체계이론가인 탈코트 파슨스를 문화이론적 관점에서 재구성 함으로써 디자인과 문화, 그리고 기능의 문제에 접근하는 문화 연구의 경로와 개념적 지도의 한 예를 제시한다. 디자인의 문화적 이해를 위한 사회이론적 탐구의 대상으로 파슨스를 선택한 이유 - 따라서 이 논문이 성취할 수 있는 잠재적인 이론적 성과 - 는 다음과 같다. 첫째, ``기능주의``를 통해 디자인이론과 사회이론을 연결함으로써 디자인의 실제적(경제적, 기술적) 기능뿐 아니라 미적 기능과 상징적 기능을 설명함과 동시에 디자인의 문화적 해석의 중요성을 제기하기 위해서이다. 사실상 기능에 대한 담론은 디자인사의 중심을 차지하여 왔으며, 이러한 담론의 배후에는 디자인의 실제적(경제적, 기술적) 기능과 미적 기능, 그리고 상징적기능 사이의 긴장이 존재한다. 둘째, 인간은 합리적이라는 전제에서 출발하여 경제적 차원의 설명만을 제공하는 디자인의 기능주의의 한계를 극복하는 데 있어서 기능적 사회이론의 도입이 지닌 유용성이다. 사회이론의 기능주의는 체계이론과 밀접하게 결합됨으로써 경제, 정치, 사회 체계와의 관련 속에서 문화체계를 규명할 뿐 아니라, ``경제적 인간`` 모델을 비판하고 인간 행위의 규범적 차원을 강조함으로써 문화이론적 함의를 풍부하게 내포한다. 셋째, 한국의 디자인학계, 특히 이들의 문화에 대한 이론적 고찰에서 빈번이 인용되는 트롬페나스(Trompenaars, 1994)나 호프스테드(Hofstede, 2010)의 문화변수 모델들의 원형은 파슨스가 그의 ``유형변수`` 모델을 통하여 이미 제시한 것이다. 그럼에도 불구하고 디자인의 문화연구는 기능적인 변수들의 유형화와 계량화에 집중함으로써 유형변수들을 활용하여 사회변동과 사회변동에서의 문화의 역할을 조명하려는 파슨스의 의도를 왜곡하여 적용한다. 이상의 이론적 공헌을 겨냥하면서, 나는 디자인의 기능주의, 인본주의를 지향하는 조형이론인 ``확장된 기능주의(extended functionalism)``, 그리고 파슨스로 대표되는 기능적 사회이론의 역사적 배경에 대한 설명으로부터 시작할 것이다. 그리고 나서 파슨스의 이론을 문화적 관점에서 재구성하여 분석할 것이다. 마지막으로 디자인의 문화적 이해에 대해 파슨스의 문화이론이 지니는 함의를 진단하면서 논문을 마무리할 것이다.},
  annotation = {00000},
  file = {D:\yonggeun\porter\zotero\storage\B5UAFX4U\강 - 2011 - 디자인의 문화적 이해를 위한 사회이론적 탐구 파슨스의 문화이론을 통해서 본 디자인과 문화.pdf}
}

