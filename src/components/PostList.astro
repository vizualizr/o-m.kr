---
import { undefined } from "astro:schema";

const allPosts = Object.values(
    import.meta.glob("/src/pages/posts/*.(html|HTML|md|astro)", { eager: true }),
);
---

<ul>
    {
        allPosts.map((post: any) => (
            <li>
                <a href={post.url} > {post.url} {}
                </a>
            </li>
        ))
    }
</ul>
